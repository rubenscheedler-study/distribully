package distribully.model;

public class Card {
	private int number;
	private CardSuit suit;
	
	public Card(int number, CardSuit suit) {
		this.setNumber(number);
		this.setSuit(suit);
	}

	private int getNumber() {
		return number;
	}

	private void setNumber(int number) {
		this.number = number;
	}

	private CardSuit getSuit() {
		return suit;
	}

	private void setSuit(CardSuit suit) {
		this.suit = suit;
	}
	
	
}
